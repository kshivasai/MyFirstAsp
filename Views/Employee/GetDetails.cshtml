@model MyFirstAsp.Models.Employee
@{
    ViewBag.Title = "Employee Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Employee Details</h2>
<table>
    <tr>
        <td>
            <b>Employee Id  :</b>
        </td>
        <td>
            @Model.EmployeeId
        </td>
    </tr>
    <tr>
        <td>
            <b>Name is  :</b>
        </td>
        <td>
            @Model.Name
        </td>
    </tr>
    <tr>
        <td>
            <b>Gender   :</b>
        </td>
        <td>
            @Model.Gender
        </td>
    </tr>

</table>



